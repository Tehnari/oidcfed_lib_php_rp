{
    "name": "tehnari/oidcfed_lib_php_rp",
    "description": "This project is an attempt to create Relying Party (RP) using the OIDCFED (OpenID Connect Federation) specification, developed as a PHP language / platform library. It is in development and is currently not recommended for use in production.",
    "type": "library",
    "require": {
        "jumbojett/openid-connect-php": "dev-master",
        "spomky-labs/jose": "^6.1@dev",
        "lcobucci/jwt": "^3.3",
        "pear/net_webfinger": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Constantin Sclifos",
            "email": "sclifcon@vle.ase.md"
        }
    ],
    "keywords": [
        "openid",
        "openid connect",
        "claim",
        "oauth",
        "oauth",
        "oauth2",
        "oauth 2",
        "oauth 2.0",
        "jwt",
        "jose",
        "auth"
    ],
    "autoload": {
        "psr-4": {
            "oidcfed\\": "classes/"
        }
    },
    "minimum-stability": "dev"
}
